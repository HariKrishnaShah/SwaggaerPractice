
{
    "entryFile": "src/server.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/controllers/*.ts"],
    "spec": {
      "outputDirectory": "build",
      "specVersion": 3
    },
    "routes": {
      "routesDir": "build"
    },
    "securityDefinitions": {
      "cookieAuth": {
          "type": "cookie",
          "name": "access_token"
      }
  }
  }