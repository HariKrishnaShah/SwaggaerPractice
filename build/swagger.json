								}
							}
						}
					},
					"401": {
						"description": "Unauthorized"
					},
					"500": {
						"description": "Internal Servel Error"
					}
				},
				"description": "Get all users",
				"summary": "Get All Users",
				"tags": [
					"User"
				],
				"security": [
					{
						"access_token": [
							"read:example"
						]
					}
				],
				"parameters": []
			}
