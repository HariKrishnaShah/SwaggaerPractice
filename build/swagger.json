		"schemas": {
			"User": {
				"properties": {
					"username": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				},
				"required": [
					"username",
					"email",
					"password"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Role": {
				"enum": [
					"ADMIN",
					"USER",
					"SUPERADMIN"
				],
				"type": "string"
			},
			"doc": {
				"properties": {
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"role": {
						"$ref": "#/components/schemas/Role"
					}
				},
				"required": [
					"createdAt",
					"role"
				],
				"type": "object",
				"additionalProperties": false
			},
			"DocUser": {
				"allOf": [
					{
						"$ref": "#/components/schemas/User"
					},
					{
						"$ref": "#/components/schemas/doc"
					}
				]
			},
			"createResult": {
				"properties": {
					"name": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"role": {
						"$ref": "#/components/schemas/Role"
					}
				},
				"required": [
					"name",
					"email",
					"role"
				],
				"type": "object",
				"additionalProperties": false
			},
			"loginResult": {
				"$ref": "#/components/schemas/createResult"
			},
			"login": {
				"properties": {
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
					}
				},
				"required": [
					"email",
					"password"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Details": {
				"properties": {
					"id": {
						"type": "number",
						"format": "double"
					},
					"name": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"name"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
